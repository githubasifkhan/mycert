<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="property_unit_type_form">
            <field name="name">property.unit.type.form</field>
            <field name="model">property.unit.type</field>
            <field name="arch" type="xml">
                <form string="Property Unit Type" >
                    <sheet>
                        <group>
                            <group>
                                <!-- <field name="code"/> -->
                                <field name="name"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="property_unit_type_tree">
            <field name="name">property.unit.type.tree</field>
            <field name="model">property.unit.type</field>
            <field name="arch" type="xml">
                    <tree>
                        <!-- <field name="code"/> -->
                        <field name="name"/>
                    </tree>
            </field>
        </record>

        <!--Facility Building menu action-->
        <record id="property_unit_type_action" model="ir.actions.act_window">
            <field name="name">Property Unit Type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">property.unit.type</field>
<!--            <field name="view_type">form</field>-->
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to add new Property Unit Type.
                </p>
            </field>
        </record>
        <menuitem id="property_unit_type_menu" name="Type" parent="property_config_menu" sequence="2" action="property_unit_type_action"/>
